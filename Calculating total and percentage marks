// to input marks in 5 subjects and print the total and average marks
#include <stdio.h>

int main() {
    int m1,m2,m3,m4,m5,total;
    float avg;
    printf("\nEnter marks in 5 subjects");
    scanf("%d%d%d%d%d",&m1,&m2,&m3,&m4,&m5);
    total = m1+m2+m3+m4+m5;
    avg=total/5.0;
    printf("\nTotal Marks %d:",total);
    printf("\nPercentage Marks %.2f:",avg);
    return 0;
}
