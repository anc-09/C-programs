// program to find greatest of 3 numbers
#include <stdio.h>

int main() {
    int a,b,c;
    printf("Enter three number:");
    scanf("%d %d %d",&a,&b,&c);
    
    if (a>b)
    {
        if (a>c)
        printf("The Greatest Number is %d",a);
        else
        printf("The Greatest Number is %d",c);
        
    }
    else
    {
        if(b>c)
        printf("The Greatest Number is %d",b);
        else
        printf("The Greatest Number is %d",c);
        
    }

    return 0;
}
