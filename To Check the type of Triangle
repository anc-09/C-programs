// program to check if a triangle is equilateral,scalene or isoceles
#include <stdio.h>

int main() {
    int a,b,c;
    printf("Enter the three sides of a triangle");
    scanf("%d %d %d",&a,&b,&c);
    if (a==b&&b==c)
    {
     printf("Triangle is Equilateral");   
    }
    else
    if (a==b||b==c||c==a)
    printf("Triangle is Isoceles");
    else
    printf("Triangle is Scalene");
    return 0;
}
